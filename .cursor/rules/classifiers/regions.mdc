export type RegionCode = keyof typeof REGIONS;

/**
 * Валидация кода района/города
 */
export function validateRegionCode(code: string): boolean {
  // Проверка формата: 7 цифр, начинается с 17
  if (!/^17\d{5}$/.test(code)) {
    return false;
  }
  return code in REGIONS;
}

/**
 * Получение региона по коду
 */
export function getRegionByCode(code: string): Region | undefined {
  return REGIONS[code];
}

/**
 * Получение списка районов/городов по коду региона
 */
export function getRegionsByRegionCode(regionCode: string): Region[] {
  return Object.values(REGIONS).filter(r => r.regionCode === regionCode);
}

/**
 * Поиск районов/городов по названию
 */
export function searchRegions(query: string): Region[] {
  const lowerQuery = query.toLowerCase();
  return Object.values(REGIONS).filter(r => 
    r.name.toLowerCase().includes(lowerQuery)
  );
}
